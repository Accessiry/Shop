<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dhee.mapper.MessageMapper">
    <!--添加留言-->
    <insert id="addMyMessage">
        INSERT INTO MESSAGE
        (TITLE,CONTENT,DATE,MID)
        VALUES (#{title},#{content},#{date,jdbcType=TIMESTAMP},#{mid})
    </insert>
</mapper>